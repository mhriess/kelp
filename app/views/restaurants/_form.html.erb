<%= form_for @restaurant do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	
	<%= f.label :neighborhood %>
	<%= f.text_field :neighborhood %>
	
	<%= f.label :address %>
	<%= f.text_field :address %>
	
	<%= f.label :city %>
	<%= f.text_field :city %>
	
	<%= f.label :state %>
	<%= f.text_field :state %>
	
	<%= f.label :zipcode %>
	<%= f.text_field :zipcode %>
	
	<%= f.submit "Add restaurant" %>
	
<% end %>